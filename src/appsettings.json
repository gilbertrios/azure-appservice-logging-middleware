{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "ApplicationInsights": {
    "ConnectionString": ""
  },
  "ObfuscationMiddleware": {
    "Enabled": true,
    "ObfuscationMask": "***REDACTED***",
    "SensitiveProperties": [
      "password",
      "creditCard",
      "creditCardNumber",
      "cardNumber",
      "cvv",
      "ssn",
      "socialSecurityNumber",
      "secret",
      "apiKey",
      "token",
      "authorization"
    ]
  }
}
